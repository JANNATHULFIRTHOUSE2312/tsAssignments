type products ={
    name:string;
    price:number;
}
const pro:products[]=[
    {name:"Laptop", price:1000},
    {name:"Phone", price:500},
    {name:"Tablet", price:300}
]
function getproducts(pro:products[]):string[]{
    return pro.map(product => `${product.name} - $${product.price}`);
}
console.log(getproducts(pro));
